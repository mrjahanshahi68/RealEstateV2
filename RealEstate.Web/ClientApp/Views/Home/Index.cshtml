							<img src="/@item.CoverImagePath" class="cover-image" />
							<div class="label">@item.PropertyTypeName</div>
						</div>
                        <div class="pi-text">
                            @if (item.Type == AppEnums.TransactionType.Rent)
                            {
                                if (item.MortgagePrice.HasValue)
