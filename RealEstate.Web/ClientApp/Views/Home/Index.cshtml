<style>
	.hs-item .hc-inner-text .hc-widget li {
		font-size: 15px;
		color: #111111;
		font-weight: 500;
		list-style: none;
		width: 33%;
		float: right;
		margin-bottom: 20px;
	}
	.hs-item .hc-inner-text .hc-text {
		float: unset;
		margin-left: 18px;
	}
	.hs-item .hc-inner-text .hc-widget {
		overflow: hidden;
		width: 100%;
		position: relative;
		left: auto;
	}

